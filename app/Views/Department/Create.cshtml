@model app.Models.Department
@{
    ViewData["Title"] = "Nuevo Departamento";
}
<h2>Nuevo Departamento</h2>
<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="DeptNo"></label>
        <input asp-for="DeptNo" class="form-control" />
        <span asp-validation-for="DeptNo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DeptName"></label>
        <input asp-for="DeptName" class="form-control" />
        <span asp-validation-for="DeptName" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-success">Guardar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
